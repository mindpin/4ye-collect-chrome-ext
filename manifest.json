{
  "manifest_version": 2,

  "name": "4ye Web URL Collector",
  "description": "URL collecting tool.",
  "version": "0.0.2",

  "permissions": [
    "tabs",
    "<all_urls>",
    "activeTab",
    "identity",
    "storage"
  ],
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "web_accessible_resources": [
    "*"
  ],
  
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
